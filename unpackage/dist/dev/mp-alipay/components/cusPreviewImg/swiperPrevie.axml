<block a:if="{{showBox}}"><view data-event-opts="{{[['touchMove',[['',['$event']]]]]}}" class="previewImg data-v-1a83fe56" catchTouchMove="__e"><view data-event-opts="{{[['tap',[['close',['$event']]]]]}}" class="mask data-v-1a83fe56" onTap="__e"><swiper class="mask-swiper data-v-1a83fe56" current="{{currentIndex}}" circular="{{circular}}" duration="{{duration}}" data-event-opts="{{[['change',[['changeSwiper',['$event']]]]]}}" onChange="__e"><swiper-item class="flex flex-column justify-center align-center data-v-1a83fe56" a:for="{{list}}" a:for-item="src" a:for-index="i" a:key="i"><image class="mask-swiper-img data-v-1a83fe56" src="{{src}}" mode="widthFix"></image></swiper-item></swiper></view><block a:if="{{list.length>0}}"><view class="pagebox data-v-1a83fe56">{{$root.m0+1+" / "+list.length}}</view></block></view></block>