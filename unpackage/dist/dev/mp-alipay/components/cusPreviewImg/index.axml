<block a:if="{{showBox}}"><view data-event-opts="{{[['touchMove',[['',['$event']]]]]}}" class="previewImg data-v-9a5be2d8" catchTouchMove="__e"><view data-event-opts="{{[['tap',[['close',['$event']]]]]}}" class="mask data-v-9a5be2d8" onTap="__e"><swiper class="mask-swiper data-v-9a5be2d8" current="{{currentIndex}}" circular="{{circular}}" duration="{{duration}}" data-event-opts="{{[['change',[['changeSwiper',['$event']]]]]}}" onChange="__e"><swiper-item class="flex flex-column justify-center align-center data-v-9a5be2d8" a:for="{{$root.l0}}" a:for-item="src" a:for-index="i" a:key="i"><image class="mask-swiper-img data-v-9a5be2d8" src="{{src.$orig.image}}" mode="widthFix"></image><view class="mask_sku data-v-9a5be2d8"><text class="sku_name data-v-9a5be2d8">{{src.$orig.suk}}</text><text class="sku_price data-v-9a5be2d8">{{src.m0+src.$orig.price}}</text></view></swiper-item></swiper></view><block a:if="{{list.length>0}}"><view class="pagebox data-v-9a5be2d8">{{$root.m1+1+" / "+list.length}}</view></block></view></block>