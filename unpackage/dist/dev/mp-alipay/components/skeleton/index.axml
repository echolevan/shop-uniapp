<block a:if="{{show}}"><view style="{{'width:'+(systemInfo.width+'px')+';'+('height:'+(systemInfo.height+'px')+';')+('background-color:'+(bgcolor)+';')+('position:'+('absolute')+';')+('left:'+(0)+';')+('top:'+(0)+';')+('z-index:'+(9998)+';')}}"><view class="{{(loading=='chiaroscuro'?'chiaroscuro':'')}}" style="{{'width:'+(item.width+'px')+';'+('height:'+(item.height+'px')+';')+('background-color:'+('rgb(194, 207, 214,.3)')+';')+('position:'+('absolute')+';')+('left:'+(item.left+'px')+';')+('top:'+(item.top+'px')+';')}}" a:for="{{skeletonRectLists}}" a:for-item="item" a:for-index="rect_idx"></view><view class="{{(loading=='chiaroscuro'?'chiaroscuro':'')}}" style="{{'width:'+(item.width+'px')+';'+('height:'+(item.height+'px')+';')+('background-color:'+('rgb(194, 207, 214,.3)')+';')+('border-radius:'+(item.width+'px')+';')+('position:'+('absolute')+';')+('left:'+(item.left+'px')+';')+('top:'+(item.top+'px')+';')}}" a:for="{{skeletonCircleLists}}" a:for-item="item" a:for-index="circle_idx"></view><block a:if="{{loading=='spin'}}"><view class="spinbox"><view class="spin"></view></view></block></view></block>