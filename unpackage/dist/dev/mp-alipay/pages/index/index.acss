.page {
  overflow-y: scroll;
  overflow-x: hidden;
}
.myApplet {
  position: relative;
}
.myApplet::after {
  position: absolute;
  right: 55px;
  top: -5px;
  content: '';
  width: 0;
  height: 0;
  border-left: 7px solid transparent;
  border-right: 7px solid transparent;
  border-bottom: 7px solid #fff;
}
