  top: -15rpx;
  font-size: 20rpx;
  padding: 0 8rpx;
  border: 1px solid var(--view-theme);
}
.new-users .support {
  width: 219rpx;
  height: 74rpx;
  margin: 54rpx auto;
  display: block;
}
