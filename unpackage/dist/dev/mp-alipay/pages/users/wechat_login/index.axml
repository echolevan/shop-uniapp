<view class="wrapper" style="{{(colorStyle)}}"><view class="bag"><image src="{{'../static/login-bg_'+colorStatus+'.jpg'}}" alt srcset class="_img"></image></view><view class="system-height" style="{{'height:'+(statusBarHeight)+';'}}"></view><view class="merchant-msg"><image src="{{configData.wap_login_logo}}" class="_img"></image><view class="name">{{''+configData.site_name+''}}</view></view><view class="wechat_login"><view class="btn-wrapper"><block a:if="{{configData.wechat_auth_switch}}"><button class="bg-theme btn1" open-type="getAuthorize" scope="phoneNumber" data-event-opts="{{[['getAuthorize',[['$onAliGetAuthorize',['getphonenumber','$event']]]],['error',[['$onAliAuthError',['getphonenumber','$event']]]]]}}" onGetAuthorize="__e" onError="__e">{{$root.m0+''}}</button></block></view></view><block a:if="{{!canGetPrivacySetting}}"><view class="protocol"><checkbox-group data-event-opts="{{[['tap',[['ChangeIsDefault',['$event']]]]]}}" catchTap="__e"><checkbox class="{{(inAnimation?'trembling':'')}}" checked="{{protocol?true:false}}" data-event-opts="{{[['animationEnd',[['e0',['$event']]]]]}}" onAnimationEnd="__e"></checkbox><text data-event-opts="{{[['tap',[['ChangeIsDefault',['$event']]]]]}}" catchTap="__e">{{$root.m1}}</text><text data-event-opts="{{[['tap',[['privacy',[4]]]]]}}" class="main-color" catchTap="__e">{{$root.m2}}</text>{{''+$root.m3+''}}<text data-event-opts="{{[['tap',[['privacy',[3]]]]]}}" class="main-color" catchTap="__e">{{$root.m4}}</text></checkbox-group></view></block><block a:if="{{isUp}}"><block><mobile-login vue-id="42463e49-1" isUp="{{isUp}}" canClose="{{canClose}}" authKey="{{authKey}}" data-event-opts="{{[['^close',[['maskClose']]],['^wechatPhone',[['wechatPhone']]]]}}" onClose="__e" onWechatPhone="__e" onVueInit="__l"></mobile-login></block></block><block a:if="{{isPhoneBox}}"><block><routine-phone vue-id="42463e49-2" logoUrl="{{logoUrl}}" isPhoneBox="{{isPhoneBox}}" authKey="{{authKey}}" data-event-opts="{{[['^loginSuccess',[['bindPhoneClose']]]]}}" onLoginSuccess="__e" onVueInit="__l"></routine-phone></block></block><block><edit-user-modal vue-id="42463e49-3" isShow="{{isShow}}" data-event-opts="{{[['^closeEdit',[['closeEdit']]],['^editSuccess',[['editSuccess']]]]}}" onCloseEdit="__e" onEditSuccess="__e" onVueInit="__l"></edit-user-modal></block><block a:if="{{canGetPrivacySetting}}"><privacy-agreement-popup onOnReject="__e" onOnAgree="__e" vue-id="42463e49-4" data-event-opts="{{[['^onReject',[['onReject']]],['^onAgree',[['onAgree']]]]}}" onVueInit="__l"></privacy-agreement-popup></block></view>