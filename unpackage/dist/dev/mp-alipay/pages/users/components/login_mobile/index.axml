<block a:if="{{isUp}}"><view><view data-event-opts="{{[['tap',[['close',['$event']]]]]}}" class="mobile-bg" onTap="__e"></view><view class="{{((('mobile-mask')+' '+'animated')+' '+((isUp)?'slideInUp':''))}}"><view class="input-item"><input type="text" placeholder="{{$root.m0}}" maxlength="11" data-event-opts="{{[['input',[['__set_model',['','account','$event',[]]]]]]}}" value="{{account}}" onInput="__e"/></view><view class="input-item"><input type="text" placeholder="{{$root.m1}}" maxlength="6" data-event-opts="{{[['input',[['__set_model',['','codeNum','$event',[]]]]]]}}" value="{{codeNum}}" onInput="__e"/><button class="code" disabled="{{disabled}}" data-event-opts="{{[['tap',[['code',['$event']]]]]}}" onTap="__e">{{text}}</button></view><view data-event-opts="{{[['tap',[['loginBtn',['$event']]]]]}}" class="sub_btn" onTap="__e">{{$root.m2}}</view></view><verify vue-id="79fe58b8-1" captchaType="{{captchaType}}" imgSize="{{({width:'330px',height:'155px'})}}" ref="__r" data-ref="verify" data-event-opts="{{[['^success',[['success']]]]}}" onSuccess="__e" onVueInit="__l"></verify></view></block>